{
  "name": "multiproxy",
  "version": "1.0.0",
  "description": "Proxy for multi destinations",
  "main": "src/main/app.js",
  "scripts": {
    "start": "$(npm bin)/nodemon --exec babel-node --presets es2015,stage-2 src/main/app.js",
    "test": "$(npm bin)/mocha --compilers js:babel-register --recursive src/test",
    "debug-test": "$(npm bin)/mocha debug --compilers js:babel-register --recursive src/test",
    "coverage": "$(npm bin)/babel-node $(npm bin)/babel-istanbul cover $(npm bin)/_mocha -- --recursive src/test -R spec"
  },
  "keywords": [
    "proxy"
  ],
  "babel": {
    "presets": ["es2015", "stage-2"]
  },
  "author": "mookjp",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-istanbul": "^0.5.9",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "mocha": "^2.3.4",
    "node-inspector": "^0.12.5",
    "nodemon": "^1.8.1",
    "request": "^2.67.0"
  },
  "dependencies": {
    "bluebird": "^3.0.6",
    "request": "^2.67.0",
    "underscore": "^1.8.3"
  }
}
